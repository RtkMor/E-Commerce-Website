{
    "version": 2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/api/(.*)",
        "dest": "backend/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
}  